<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite default-match-type="wildcard">
	<rule>
		<from>/images/**</from>
		<to>/resources/images/$1</to>
	</rule>
	<rule>
		<from>/css/**</from>
		<to>/resources/css/$1</to>
	</rule>
	<rule>
		<from>/js/**</from>
		<to>/resources/js/$1</to>
	</rule>
	<rule>
		<from>/fonts/**</from>
		<to>/resources/fonts/$1</to>
	</rule>
	<rule>
		<from>/**</from>
		<to>/app/$1</to>
	</rule>
</urlrewrite>